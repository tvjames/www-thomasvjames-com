---
layout: null
---

<ul class="side-nav">
<% year = -1 %>
<% for post in @data.query().sortCollection([{date: -1}]).toJSON(): %>
<%   date = @moment(post.date).utc() %>
<%   if date.year() != year: %>
<%   year = date.year() %>
  <li class="heading"><%= date.year() %></li>
<%   end %>
  <li>

    <a href="<%= post.url %>" title="<%= post.title %>"><span class="secondary radius label right"><%= date.format("MMMM Do YYYY") %></span><%= post.title %></a>
  </li>
<% end %>

</ul>
